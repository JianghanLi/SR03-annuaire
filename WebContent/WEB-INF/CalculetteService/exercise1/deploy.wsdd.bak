<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from CalculetteService WSDL service -->

  <service name="Calculette" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://exercise1"/>
      <parameter name="wsdlServiceElement" value="CalculetteService"/>
      <parameter name="schemaQualified" value="http://exercise1"/>
      <parameter name="wsdlServicePort" value="Calculette"/>
      <parameter name="className" value="exercise1.CalculetteSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="Calculette"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="mul" qname="operNS:mul" xmlns:operNS="http://exercise1" returnQName="retNS:mulReturn" xmlns:retNS="http://exercise1" returnType="rtns:int" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:val1" xmlns:pns="http://exercise1" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:val2" xmlns:pns="http://exercise1" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="add" qname="operNS:add" xmlns:operNS="http://exercise1" returnQName="retNS:addReturn" xmlns:retNS="http://exercise1" returnType="rtns:int" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:val1" xmlns:pns="http://exercise1" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:val2" xmlns:pns="http://exercise1" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <operation name="sou" qname="operNS:sou" xmlns:operNS="http://exercise1" returnQName="retNS:souReturn" xmlns:retNS="http://exercise1" returnType="rtns:int" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:val1" xmlns:pns="http://exercise1" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:val2" xmlns:pns="http://exercise1" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="add mul sou"/>

  </service>
</deployment>
